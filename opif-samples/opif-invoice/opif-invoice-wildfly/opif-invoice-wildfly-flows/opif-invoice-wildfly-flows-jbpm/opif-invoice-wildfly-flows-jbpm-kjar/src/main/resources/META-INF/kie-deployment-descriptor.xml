<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<deployment-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                       xsi:schemaLocation="">
    <!-- <persistence-unit>org.jbpm.domain2</persistence-unit>
    <audit-persistence-unit>org.jbpm.domain2</audit-persistence-unit> -->
    <audit-mode>NONE</audit-mode>
    <persistence-mode>NONE</persistence-mode>
    <runtime-strategy>SINGLETON</runtime-strategy>
    <marshalling-strategies/>
    <event-listeners/>
    <task-event-listeners/>
    <globals/>
	<work-item-handlers> 
		<work-item-handler>
			<resolver>mvel</resolver>
			<identifier>new pl.softfly.opif.samples.invoice.jbpm.workitemhandler.TransformStringToDocumentWorkItemHandler()</identifier>
			<parameters />
			<name>TransformStringToDocument</name>
		</work-item-handler>
	</work-item-handlers>
    <environment-entries/>
    <configurations/>
    <required-roles/>
    <remoteable-classes/>
    <limit-serialization-classes>true</limit-serialization-classes>
</deployment-descriptor>
